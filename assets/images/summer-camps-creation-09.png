			<script>document.addEventListener("DOMContentLoaded", function() {
  setTimeout(function() {
    // Získa aktuálnu cestu URL
    var path = window.location.pathname;

    // Ak URL nezačína jazykovým prefixom (napr. /sk/, /de/, /fr/ ...),
    // ber to ako anglickú verziu (hlavnú doménu)
    var isEnglish = !/^\/(sk|de|fr|es|it|pl|hu|cs|ro)\b/.test(path);

    // Vyber všetky .header-cont
    var headers = document.querySelectorAll(".header-cont");

    headers.forEach(function(header) {
      if (isEnglish) {
        header.style.display = "block";  // zobraz EN (hlavná doména + EN podstránky)
      } else {
        header.style.display = "none";   // skryj ostatné jazyky
      }
    });
  }, 400);
});
