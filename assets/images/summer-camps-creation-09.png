    </div>
    

    <script>
        document.addEventListener('DOMContentLoaded', function() {
            const container = document.querySelector('.container');
            const beforeImage = document.querySelector('.before-image');
            const sliderControl = document.querySelector('.slider-control');
            
            let isDragging = false;
            
            // Helper function to handle the slider movement
            function moveSlider(clientX) {
                const containerRect = container.getBoundingClientRect();
                let position = (clientX - containerRect.left) / containerRect.width;
                
                // Clamp position between 0 and 1
                position = Math.max(0, Math.min(1, position));
                
                // Update slider position
                sliderControl.style.left = position * 100 + '%';
                
                // Update before image clip path
                beforeImage.style.clipPath = `inset(0 ${(1 - position) * 100}% 0 0)`;
            }
            
            // Mouse events
            sliderControl.addEventListener('mousedown', (e) => {
                isDragging = true;
                e.preventDefault(); // Prevent text selection
            });
            
            document.addEventListener('mousemove', (e) => {
                if (isDragging) {
                    moveSlider(e.clientX);
                }
            });
            
            document.addEventListener('mouseup', () => {
                isDragging = false;
            });
            
            // Touch events
            sliderControl.addEventListener('touchstart', (e) => {
                isDragging = true;
                e.preventDefault(); // Prevent default touch behavior
            });
            
            document.addEventListener('touchmove', (e) => {
                if (isDragging) {
                    moveSlider(e.touches[0].clientX);
                    e.preventDefault(); // Prevent scrolling while dragging
                }
            });
            
            document.addEventListener('touchend', () => {
                isDragging = false;
            });
            
            // Click anywhere on the container to move the slider
            container.addEventListener('click', (e) => {
                if (e.target !== sliderControl && !sliderControl.contains(e.target)) {
                    moveSlider(e.clientX);
                }
            });
            
            // Handle window resize
            window.addEventListener('resize', () => {
                // Recalculate position based on current slider position
                const position = parseFloat(sliderControl.style.left) / 100 || 0.5;
                sliderControl.style.left = position * 100 + '%';
                beforeImage.style.clipPath = `inset(0 ${(1 - position) * 100}% 0 0)`;
            });
            
            // Initialize slider at center
            moveSlider(container.getBoundingClientRect().left + container.offsetWidth / 2);
        });
    </script>
